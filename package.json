{
  "name": "@metaxia/scriptures-source-byztxt-tr",
  "version": "0.1.0",
  "description": "Byzantine Textus Receptus (Greek) data for @metaxia/scriptures. Source: Byzantine Text Project.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "import": "./dist/register.js"
    },
    "./metadata": "./data/byztxt-TR/metadata.json",
    "./data/*": "./data/*",
    "./cache/*": "./cache/*",
    "./books/*": {
      "types": "./dist/books/*.d.ts",
      "import": "./dist/books/*.js"
    }
  },
  "files": [
    "dist",
    "data",
    "cache"
  ],
  "scripts": {
    "build": "tsc",
    "import": "tsx scripts/import.ts",
    "test": "vitest run",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "bible",
    "greek",
    "textus-receptus",
    "byzantine",
    "scriptures",
    "new-testament"
  ],
  "author": "Benjamin Plunkert",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/metaxiamultimedia/scriptures-js-source-byztxt-tr"
  },
  "bugs": {
    "url": "https://github.com/metaxiamultimedia/scriptures-js-source-byztxt-tr/issues"
  },
  "homepage": "https://github.com/metaxiamultimedia/scriptures-js-source-byztxt-tr#readme",
  "peerDependencies": {
    "@metaxia/scriptures-core": ">=0.1.0"
  },
  "devDependencies": {
    "@metaxia/scriptures-core": "git+ssh://git@github.com/metaxiamultimedia/scriptures-js-core",
    "@types/unzipper": "^0.10.9",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "unzipper": "^0.12.0",
    "vitest": "^1.0.0"
  }
}
